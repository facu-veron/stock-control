{
  "name": "inventario-backend",
  "version": "1.0.0",
  "description": "Backend para sistema de inventario en TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node src/seed.ts",
    "db:studio": "prisma studio",
    "type-check": "tsc --noEmit --skipLibCheck"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=11.0.0"
  },
  "keywords": [
    "inventory",
    "express",
    "prisma",
    "typescript",
    "sqlite"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "-": "^0.0.1",
    "@prisma/client": "^6.16.1",
    "@types/multer": "^2.0.0",
    "afip.ts": "^3.2.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.10.5",
    "prisma": "^6.16.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
