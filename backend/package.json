{
  "name": "inventario-backend",
  "version": "1.0.0",
  "description": "Backend para sistema de inventario en TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node src/seed.ts",
    "db:studio": "prisma studio",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "inventory",
    "express",
    "prisma",
    "typescript",
    "sqlite"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "-": "^0.0.1",
    "@prisma/client": "^6.13.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "fast-xml-parser": "^5.2.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-forge": "^1.3.1",
    "save-dev": "^0.0.1-security",
    "soap": "^1.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "prisma": "^6.13.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
